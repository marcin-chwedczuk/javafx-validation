<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" stylesheets="@Demo.css" xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.marcinchwedczuk.javafx.validation.demo.Demo">
    <children>
        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0" styleClass="parent">
            <children>
                <TitledPane fx:id="userRegistrationPane" animated="false" text="Username &amp; Password"
                            VBox.vgrow="NEVER" />

                <TitledPane fx:id="numberRangePane" animated="false" text="Number range..."
                            VBox.vgrow="NEVER" />

                <TitledPane fx:id="topDownPane" animated="false" text="Top down validation..."
                            VBox.vgrow="NEVER" />
            </children>
        </VBox>
    </children>
</AnchorPane>
